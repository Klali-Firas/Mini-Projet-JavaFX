<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" styleClass="material-pane" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="l2dsi2.firas.miniprojetfx.Controller.AddAdminController">
    <stylesheets>
        <URL value="@styles.css" />
    </stylesheets>
    <children>
        <Label layoutX="211.0" layoutY="39.0" styleClass="material-label" text="Username:" />
        <TextField fx:id="usernameField" layoutX="211.0" layoutY="57.0" prefHeight="27.0" prefWidth="203.0" styleClass="material-input" />

        <Label layoutX="209.0" layoutY="107.0" styleClass="material-label" text="Password:" />
        <PasswordField fx:id="passwordField" layoutX="211.0" layoutY="125.0" prefHeight="27.0" prefWidth="203.0" styleClass="material-input" />

        <Label layoutX="210.0" layoutY="171.0" styleClass="material-label" text="Login:" />
        <TextField fx:id="loginField" layoutX="210.0" layoutY="189.0" prefHeight="27.0" prefWidth="203.0" styleClass="material-input" />

        <Label layoutX="210.0" layoutY="235.0" styleClass="material-label" text="Telephone:" />
        <TextField fx:id="telephoneField" layoutX="211.0" layoutY="253.0" prefHeight="27.0" prefWidth="203.0" styleClass="material-input" />

        <Button fx:id="submitButton" layoutX="211.0" layoutY="307.0" onAction="#handleSubmitButtonAction" styleClass="material-button" text="Ajouter" />
      <Button fx:id="Annuler" layoutX="334.0" layoutY="307.0" onAction="#handleSubmitButtonAction" styleClass="material-button" text="Annuler" />
    </children>
</AnchorPane>
