<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.text.Font?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="l2dsi2.firas.miniprojetfx.Controller.AddPatientController"
            prefHeight="400.0" prefWidth="600.0">
    <children>
        <TextField fx:id="nomInput" layoutX="200.0" layoutY="49.0">
            <font>
                <Font size="16.0" />
            </font>
        </TextField>
        <TextField fx:id="prenomInput" layoutX="200.0" layoutY="113.0">
            <font>
                <Font size="16.0" />
            </font>
        </TextField>
        <TextField fx:id="telInput" layoutX="200.0" layoutY="181.0">
            <font>
                <Font size="16.0" />
            </font>
        </TextField>
        <Label layoutX="200.0" layoutY="32.0" text="Nom" />
        <Label layoutX="200.0" layoutY="96.0" text="Prenom" />
        <Label layoutX="200.0" layoutY="165.0" text="Telephone" />
        <Label layoutX="200.0" layoutY="228.0" text="Birthday" />
        <DatePicker fx:id="birthdayInput" layoutX="200.0" layoutY="246.0" prefHeight="37.0" prefWidth="199.0" />
        <Button fx:id="add" layoutX="200.0" layoutY="317.0" mnemonicParsing="false" text="Add">
            <font>
                <Font size="14.0" />
            </font>
        </Button>
        <Button fx:id="cancel" layoutX="338.0" layoutY="317.0" mnemonicParsing="false" text="Cancel">
            <font>
                <Font size="14.0" />
            </font>
        </Button>
    </children>
</AnchorPane>
