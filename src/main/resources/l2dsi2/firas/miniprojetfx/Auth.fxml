<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" styleClass="material-pane" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="l2dsi2.firas.miniprojetfx.Controller.AuthController">
   <stylesheets>
      <URL value="@styles.css" />
   </stylesheets>
   <children>
      <Button fx:id="loginButton" layoutX="258.0" layoutY="311.0" mnemonicParsing="false" onAction="#login" styleClass="material-button" text="Login">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <Label fx:id="label" alignment="CENTER" contentDisplay="CENTER" layoutX="176.0" layoutY="62.0" prefHeight="38.0" prefWidth="247.0" styleClass="material-label" text="Se Conneter" textAlignment="CENTER">
         <font>
            <Font name="System Bold" size="26.0" />
         </font>
      </Label>
      <TextField fx:id="loginInput" layoutX="200.0" layoutY="163.0" styleClass="material-input">
         <font>
            <Font size="16.0" />
         </font>
      </TextField>
      <PasswordField fx:id="passInput" accessibleRole="PASSWORD_FIELD" layoutX="199.0" layoutY="244.0" styleClass="material-input">
         <font>
            <Font size="16.0" />
         </font>
      </PasswordField>
      <Label layoutX="200.0" layoutY="135.0" styleClass="material-label" text="Login">
         <font>
            <Font size="16.0" />
         </font></Label>
      <Label layoutX="199.0" layoutY="216.0" styleClass="material-label" text="Password">
         <font>
            <Font size="16.0" />
         </font></Label>

   </children>
</AnchorPane>
